<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.fom.boot.domain.member.model.mapper.MamberMapper">
  	
  	<select id="selectOneByLogin" parameterType="String" resultType="Member">
  		SELECT 
  		MEMBER_ID,
  		MEMBER_PW,
        MEMBER_NAME,
        MEMBER_NICKNAME,
        EMAIL,
        PHONE,
        GENDER,
        ENROLL_DATE,
        STATUS_YN,
        ADMIN_YN
  		FROM MEMBER
  		WHERE MEMBER_ID = #{memberId}
  	</select>
  </mapper>